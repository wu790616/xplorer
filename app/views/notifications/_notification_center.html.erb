<ul class="notificator">
  <a href="#" id="open_notification">
    <%= render 'notifications/counter', counter: notifications.count %>
  </a>
  <div id="notificationContainer">
    <div id="notificationTitle">通知</div>
    <div id="notificationsBody" class="notifications">
      <ul class="notification-list" id="notificationList">
        <% notifications.each do |notification| %>
          <%=render partial: "notifications/share_list", locals: {notification: notification} %>
        <% end %>
      </ul>
    </div>
    <div id="notificationFooter"></div>
  </div>
</ul>